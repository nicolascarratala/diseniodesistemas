package Continente;

public class Capital {

	private Provincia contiene[];
	private String Nombre_capital;
	private int año_de_fundacion;
	
	public Capital() {
	}

	}

	public Capital(Provincia contiene, String nombre_capital, int año_de_fundacion) {
		this.contiene = contiene;
		Nombre_capital = nombre_capital;
		this.año_de_fundacion = año_de_fundacion;
	}


	private String getNombre_capital() {
		return Nombre_capital;
	}

	private void setNombre_capital(String nombre_capital) {
		Nombre_capital = nombre_capital;
	}

	private int getAño_de_fundacion() {
		return año_de_fundacion;
	}

	private void setAño_de_fundacion(int año_de_fundacion) {
		this.año_de_fundacion = año_de_fundacion;
	}
	

}


