package ejercicio3;

public abstract class Pais {

	

	public Pais(){

		private int cant_habitantes;
		private char ciudad;
		private int m_MontoRecaudado;
		private int m_Gastopublico;

	}
		
	
	public char getCiudad() {
		return ciudad;
	}

	public void setCiudad(char ciudad) {
		this.ciudad = ciudad;
	}

	public int getCant_habitantes() {
		return cant_habitantes;
	}

	public void setCant_habitantes(int cant_habitantes) {
		this.cant_habitantes = cant_habitantes;
	}

	public int getM_MontoRecaudado() {
		return m_MontoRecaudado;
	}

	public void setM_MontoRecaudado(int m_MontoRecaudado) {
		this.m_MontoRecaudado = m_MontoRecaudado;
	}

	public int getM_Gastopublico() {
		return m_Gastopublico;
	}

	public void setM_Gastopublico(int m_Gastopublico) {
		this.m_Gastopublico = m_Gastopublico;
	}

	

}