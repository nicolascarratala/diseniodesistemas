package Herencia;

public class Alumno extends Persona {
	private int legajo;

	public extend getPersona() {
		return Persona;
	}

	public void setPersona(extend persona) {
		Persona = persona;
	}

	public int getLegajo() {
		return legajo;
	}

	public void setLegajo(int legajo) {
		this.legajo = legajo;
	}
	
}
