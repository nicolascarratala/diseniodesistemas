package ejercicio3;

public abstract class Pais {

	public Ciudades m_Ciudades;
	public MontoRecaudado m_MontoRecaudado;
	public Gastopublico m_Gastopublico;

	public Pais(){

	}

	public Pais(Ciudades m_Ciudades, MontoRecaudado m_MontoRecaudado, Gastopublico m_Gastopublico) {
		super();
		this.m_Ciudades = m_Ciudades;
		this.m_MontoRecaudado = m_MontoRecaudado;
		this.m_Gastopublico = m_Gastopublico;
	}

	public Ciudades getM_Ciudades() {
		return m_Ciudades;
	}

	public void setM_Ciudades(Ciudades m_Ciudades) {
		this.m_Ciudades = m_Ciudades;
	}

	public MontoRecaudado getM_MontoRecaudado() {
		return m_MontoRecaudado;
	}

	public void setM_MontoRecaudado(MontoRecaudado m_MontoRecaudado) {
		this.m_MontoRecaudado = m_MontoRecaudado;
	}

	public Gastopublico getM_Gastopublico() {
		return m_Gastopublico;
	}

	public void setM_Gastopublico(Gastopublico m_Gastopublico) {
		this.m_Gastopublico = m_Gastopublico;
	}


}