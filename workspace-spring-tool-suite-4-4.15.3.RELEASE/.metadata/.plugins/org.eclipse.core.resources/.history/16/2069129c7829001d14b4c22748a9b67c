public class Main {
	
	public static void main (String[] args) {
		
		 Pais p1 = new Pais("",5464,5465);
         Ciudades c1 = new Ciudades();
         Gastopublico gp1 = new Gastopublico();
         p1.setCiudades(c1);
         c1.setGastopublico(5465465);
         gp1.setMantenimiento(null);
         ArrayList<String> ciudades = new ArrayList<String>();
         ciudades.add("Mendoza");
         ciudades.add("San Juan");
         ciudades.add("Cordoba");
         ciudades.add("Santa Fe");
         for (String i : ciudades) {
             System.out.println("La ciudad es:"+ i.toString()+ " Su monto Recaudado es: "+p1.getMontoRecaudado()+ " Su gasto publico es de: "+gp1.());
         }
  

    

	}
}
