package Main;

import Personas.*;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Persona p1= new Persona("Mariana","Giacconi",20);
		Domicilio d1=new Domicilio("sta maria de oro", 855);
		p1.setDomi(d1);
		d1.setEsde(p1);
	System.out.println("La persona es" +": "+d1.getEsde().getNombre()+ " "+p1.getApellido());
	System.out.println("La direccion es"+": "+p1.getDomi().getCalle()+" "+ d1.getNumero());		
	}

}
